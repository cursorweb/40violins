{
  "expo": {
    "name": "40violins",
    "slug": "40violins",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logos/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logos/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/logos/favicon.png"
    },
    "description": "An all-in-one app to help with violin practice!",
    "githubUrl": "https://github.com/cursorweb/40violins"
  }
}
